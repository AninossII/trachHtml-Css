* {
    font-family: 'Lato', sans-serif;
}

header h1 {
    font-weight: bold;
    color: cyan
}

section>h1 {
    color: blue
}

li {
    color: brown;
}

ol>li:first-child {
    color: chartreuse;
}

del,
li.delete {
    color: red;
    text-decoration: none;
}

li.blackRow {
    font-size: 1.2em;
    font-weight: bolder;
    text-decoration: underline;
    color: black;
}